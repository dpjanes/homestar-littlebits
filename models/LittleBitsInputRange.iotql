CREATE MODEL LittleBitsInputRange WITH
    schema:name = "LittleBits General Control",
    iot:facet = iot-facet:toy
ATTRIBUTE on WITH
    schema:name = "on",
    iot:purpose = iot-purpose:on,
    iot:type = iot:type.boolean
ATTRIBUTE percent WITH
    schema:name = "value",
    iot:purpose = iot-purpose:value,
    iot:type = iot:type.number,
    iot:unit = iot-unit:math.fraction.percent,
    iot:minimum = 0,
    iot:maximum = 100
;
